@model AION.Web.Models.Shared.NotesViewModel

<!-- Modal Standard Notes -->
<div class="modal fade text-left" id="notes-modal" tabindex="-1" role="dialog" aria-labelledby="notes" aria-hidden="true">
    <div class="modal-dialog modal-lg" role="document">
        <div class="modal-content">
            <div class="modal-header bg-cyan white">
                <h4 class="modal-title white">Project # : @Model.Project.AccelaProjectRefId</h4>
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true">&times;</span>
                </button>
            </div>
            <div class="modal-body">
                <form class="form" id="form-notes">
                    @Html.Partial("_Notes", new AION.Web.Models.Shared.NotesViewModel() { NotesComments = Model.NotesComments })
                </form>
            </div>
            <div class="modal-footer">
                <button type="button" id="btnClose" class="btn btn-poppy" data-dismiss="modal">
                    Close
                </button>
            </div>
        </div>
    </div>
</div>


