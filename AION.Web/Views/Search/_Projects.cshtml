@model AION.Web.Models.Search.ProjectSearchResultsViewModel

<div class="row">
    <div class="col-12">
        <div class="card">
            <div class="card-header">
                <h4 class="card-title">Search Results</h4>
            </div>
            <div class="card-content collapse show">
                <div class="card-body card-dashboard">
                    <table class="table table-striped table-bordered zero-configuration fixed-header" id="projects">
                        <thead>
                            <tr>
                                <th>Date</th>
                                <th>Project #</th>
                                <th>Project Name</th>
                                <th>Project Type</th>
                                <th>Customer</th>
                                <th>Facilitator</th>
                                <th>Project Status</th>
                                <th>Meeting</th>
                            </tr>
                        </thead>
                        <tbody>
                            @foreach (AION.BL.Models.ProjectSearchResult project in Model.ProjectList)
                            {
                                <tr>
                                    <td><span style="display:none">@project.DateOfApplication.Ticks</span>@project.DateOfApplication.ToString("MM/dd/yyyy")</td>
                                    <td>
                                        <a href="@Url.Action("ProjectDetail", "ProjectDetail", new { ProjectId = project.ProjectNumber , RecIdTxt=project.RecIdTxt})">
                                            @project.ProjectNumber
                                        </a>
                                    </td>
                                    <td>
                                        <a href="@Url.Action("ProjectDetail", "ProjectDetail", new { ProjectId = project.ProjectNumber  , RecIdTxt=project.RecIdTxt})">
                                            @project.ProjectName
                                        </a>
                                    </td>
                                    <td>@project.ProjectType</td>
                                    <td>@project.CustomerName</td>
                                    <td>@project.FacilitatorName</td>
                                    <td>@project.ProjectStatus</td>
                                    <td>@project.MeetingType</td>
                                </tr>
                            }
                        </tbody>
                    </table>
                </div>
            </div>
        </div>
    </div>
</div>
