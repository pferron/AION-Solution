<!-- Modal Bulk Estimation -->
<div class="modal fade text-left" id="bulk-estimation-modal" tabindex="-1" role="dialog" aria-labelledby="myModalLabel8" aria-hidden="true">
    <div class="modal-dialog modal-lg" role="document">
        <div class="modal-content">
            <div class="modal-header bg-cyan white">
                <h4 class="modal-title white">Bulk Estimation</h4>
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true">&times;</span>
                </button>
            </div>
            <div class="modal-body p-4">
                <form class="form" id="form-bulk-estimation">
                    @{ Html.RenderAction("BulkEstimation", "Estimation");}
                </form>
            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-poppy" data-dismiss="modal">Close</button>
                <button type="button" id="btn-submit" class="btn btn-poppy">Save changes</button>
            </div>
        </div>
    </div>
</div>

