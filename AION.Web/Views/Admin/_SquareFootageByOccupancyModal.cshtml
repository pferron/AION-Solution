@model AION.Web.Models.OccupancyViewModel

<!-- Modal Square Footage by Occupancy -->
<div class="modal fade text-left" id="occupancy-square-foot-modal" tabindex="-1" role="dialog" aria-labelledby="notes" aria-hidden="true">
    <div class="modal-dialog modal-lg" role="document">
        <div class="modal-content">
            <div class="modal-header bg-cyan white">
                <h4 class="modal-title white">Square Footage By Occupancies</h4>
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true">&times;</span>
                </button>
            </div>
            <div class="modal-body">
                <form class="form" id="divSquareFootageByOccupancy">
                    @Html.Partial("_SquareFootageByOccupancy", Model)
                </form>
            </div>
            <div class="modal-footer">
                <button type="button" id="btnClose" class="btn btn-poppy" data-dismiss="modal">
                    Close
                </button>
            </div>
        </div>
    </div>
</div>

