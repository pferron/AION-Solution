@model IEnumerable<AION.Manager.Models.EventData>
    @{
        ViewBag.Title = "Calendar";
    }
    <h1>Calendar</h1>
    <table class="table">
        <thead>
            <tr>
                <th scope="col">Organizer</th>
                <th scope="col">Subject</th>
                <th scope="col">Start</th>
                <th scope="col">End</th>
                <th scope="col">Attendees</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var item in Model)
            {
                <tr>
                    <td>@item.Base.Organizer.EmailAddress.Name</td>
                    <td>@item.Base.Subject</td>
                    <td>@Convert.ToDateTime(item.Base.Start.DateTime).ToString("M/d/yy h:mm tt")</td>
                    <td>@Convert.ToDateTime(item.Base.End.DateTime).ToString("M/d/yy h:mm tt")</td>
                    <td>@item.AttendeesString</td>
                </tr>
            }
        </tbody>
    </table>
