@model List<AION.Web.Models.ConfigurationHistory.TableAuditLogViewModel>

<table class="table table-striped table-bordered table-hover dataTable" id="confighistorytable">
    <thead>
        <tr>
            <th>Date</th>
            <th>User</th>
            <th>Type</th>
            <th>Action</th>
        </tr>
    </thead>
    <tbody>
        @{ foreach (var history in Model)
            {
                <tr>
                    <td>@history.UpdatedDate</td>
                    <td>@history.UpdatedUser</td>
                    <td>@history.TypeName</td>
                    <td>@history.AuditCodeName</td>
                </tr>
            }
        }
    </tbody>
</table>
